{
  "name": "@outlit/node",
  "version": "0.2.2",
  "description": "Outlit server-side tracking SDK for Node.js",
  "license": "Apache-2.0",
  "author": "Outlit AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/OutlitAI/outlit-sdk.git",
    "directory": "packages/node"
  },
  "bugs": {
    "url": "https://github.com/OutlitAI/outlit-sdk/issues"
  },
  "homepage": "https://github.com/OutlitAI/outlit-sdk#readme",
  "keywords": [
    "outlit",
    "analytics",
    "tracking",
    "sdk",
    "node",
    "server"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "lint": "biome check . --write",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@outlit/core": "workspace:*"
  },
  "devDependencies": {
    "@outlit/typescript-config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
